{
  "compilerOptions": {
    "target": "ESNext",
    "module": "AMD",
    // "lib": ["DOM",],
    "rootDir": "./src",
    "outDir": "./dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "noEmitOnError": true
    // "isolatedModules": true
    // "downlevelIteration": true,
  }
}
